{
    "name": "chenyongze/tt-microapp",
    "description": "字节跳动小程序sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "yongze.chen",
            "email": "sapphire@gmail.com"
        }
    ],
    "require": {
        "hanson/foundation-sdk": "^4.0",
        "ext-json": "*",
        "php": ">=7.1",
        "ext-openssl": "*",
        "ext-curl": "*",
        "ext-redis": "*"
    },
    "require-dev": {
        "hyperf/utils": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Yong\\TtMicroApp\\": "src/"
